{
   "name": "node-starter",
   "version": "0.0.0",
   "scripts": {
      "dev": "nodemon --watch src --exec ts-node src/index.ts",
      "build": "npx tsc",
      "start": "npm run build && nodemon dist/index.js",
      "prettier": "prettiern '{src,/**/*.ts}' --write --single-quote --trailing-comma all"
   },
   "dependencies": {
