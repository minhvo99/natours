      "dotenv": "^16.4.5",
      "express": "^4.17.1",
      "mongodb": "^6.8.0",
      "mongoose": "^8.5.0",
      "morgan": "^1.10.0",
      "nodemon": "^3.1.4",
      "prettier": "^3.3.3",
      "ts-lib": "^0.0.5",
      "ts-node": "^10.9.2"
   },
   "devDependencies": {
      "@babel/core": "^7.24.8",
      "@babel/node": "^7.24.8",
      "@babel/preset-env": "^7.24.8",
      "@babel/preset-typescript": "^7.24.7",
      "@types/express": "^4.17.21",
      "@types/morgan": "^1.9.9",
      "@types/node": "^20.14.10",
      "typescript": "^5.5.3"
